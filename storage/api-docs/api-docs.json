    "components": {
        "schemas": {
            "Monitor": {
                "title": "Monitor",
                "required": [
                    "project_id",
                    "label",
                    "periodicity",
                    "type",
                    "status"
                ],
                "properties": {
                    "id": {
                        "type": "integer",
                        "format": "int64",
                        "example": 1
                    },
                    "project_id": {
                        "type": "integer",
                        "format": "int64",
                        "example": 1
                    },
                    "label": {
                        "type": "string",
                        "example": "Ping Google"
                    },
                    "periodicity": {
                        "description": "Interval in seconds (5-300)",
                        "type": "integer",
                        "maximum": 300,
                        "minimum": 5,
                        "example": 60
                    },
                    "monitor_type": {
                        "type": "string",
                        "example": "ping"
                    },
                    "badge_label": {
                        "type": "string",
                        "example": "Google"
                    },
                    "status": {
                        "type": "string",
                        "example": "active"
                    },
                    "latest_status": {
                        "type": "string",
                        "example": "succeeded"
                    },
                    "hostname": {
                        "type": "string",
                        "example": "google.com"
                    },
                    "port": {
                        "type": "integer",
                        "example": 80
                    },
                    "url": {
                        "type": "string",
                        "example": "https://google.com"
                    },
                    "check_status": {
                        "type": "boolean",
                        "example": true
                    },
                    "keywords": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    }
                },
                "type": "object"
            },
            "MonitorLog": {
                "title": "MonitorLog",
                "required": [
                    "monitor_id",
                    "started_at",
                    "status"
                ],
                "properties": {
                    "id": {
                        "type": "integer",
                        "format": "int64",
                        "example": 1
                    },
                    "monitor_id": {
                        "type": "integer",
                        "format": "int64",
                        "example": 1
                    },
                    "started_at": {
                        "type": "string",
                        "format": "date-time",
                        "example": "2025-08-11T12:00:00Z"
                    },
                    "status": {
                        "type": "string",
                        "example": "succeeded"
                    },
                    "response_time_ms": {
                        "type": "integer",
                        "example": 123
                    }
                },
                "type": "object"
            },
            "Project": {
                "title": "Project",
                "required": [
                    "label",
                    "user_id"
                ],
                "properties": {
                    "id": {
                        "type": "integer",
                        "format": "int64",
                        "example": 1
                    },
                    "label": {
                        "type": "string",
                        "example": "My Project"
                    },
                    "description": {
                        "type": "string",
                        "example": "Project description"
                    },
                    "tags": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    },
                    "user_id": {
                        "type": "integer",
                        "format": "int64",
                        "example": 1
                    }
                },
                "type": "object"
            }
        }
